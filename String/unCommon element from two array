#include<bits/stdc++.h>
using namespace std;
int main(){
    int a[] = {1, 4, 6, 9, 12};
    int b[] = {2, 4, 7, 8, 9, 10};
    int n1 = sizeof(a)/sizeof(a[0]);
    int n2 = sizeof(b)/sizeof(b[0]);
    int i=0,j=0;
   while(i<n1 && j<n2){
       if(a[i]<b[j]) {
           cout<<a[i++]<<" ";
       }else if(a[i]>b[j]) {
           cout<<b[j++]<<" ";
       }else{
           i++;
           j++;
       }
   }
   while(i < n1 && a[i]!=b[j]) {
      cout<<a[i++]<<" ";
   }
      
   while(j < n2 && b[j]!=a[i]) {
     cout<<b[j++]<<" ";
   }
    cout<<endl;
     return 0 ;
}
